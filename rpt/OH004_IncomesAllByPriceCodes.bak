	<queryString><![CDATA[SELECT	BLI_ITEM_GROUP, 
	BLI_ITEM_DESC, 
	SUM(BLI_QTY) AS QTY, 
	SUM(BLI_QTY * BLI_ITEM_AMOUNT) AS AMOUNT
FROM BILLITEMS
JOIN BILLS ON BLI_ID_BILL = BLL_ID
WHERE (BLL_STATUS = $P{state} or BLL_STATUS = $P{state2}) 
AND DATE(BLL_DATE) BETWEEN STR_TO_DATE($P{fromdate},'%d/%m/%Y') AND STR_TO_DATE($P{todate},'%d/%m/%Y')
GROUP BY BLI_ITEM_DESC
ORDER BY BLI_ITEM_GROUP, BLI_ITEM_DESC ASC]]></queryString>

