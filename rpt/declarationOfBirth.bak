	<queryString><![CDATA[SELECT  PDEL_DATE_DEL,
	PDEL_SEX,
	PAT_NAME,
	PDEL_CHILD_NAME,
	PDEL_FATHER_BIRTH_PLACE,
	PDEL_FATHER_AGE,
	PDEL_FATHER_RESIDENCE,
	PDEL_FATHER_OCCUPATION,
	PDEL_FATHER_NAME,
	PAT_BIRTH_PLACE,
	IF(PAT_BDATE is not null, ROUND(DATEDIFF(CURDATE(), DATE(PAT_BDATE)) / 365,0),PAT_AGE) as age,
	PAT_CITY, 
        PAT_ADDR,
        PAT_OCCUPATION,
        PAT_TELE
FROM pregnancydelivery PDEL
	JOIN admission ADM ON ( PDEL.PDEL_ADM_ID = ADM.ADM_ID ) 
	JOIN patient PAT ON    ( PAT.PAT_ID = ADM.ADM_PAT_ID ) 
WHERE PDEL.PDEL_ADM_ID = $P{admission_id}]]></queryString>

