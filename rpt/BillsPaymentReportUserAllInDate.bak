	<queryString><![CDATA[SELECT BLL_ID, BLP_DATE,  BLL_PAT_NAME,  PAID, BLP_USR_ID_A
FROM (
	SELECT BLL_ID, BLP_DATE,  BLL_PAT_NAME, IF(DATE(BLP_DATE) BETWEEN $P{fromdate} AND $P{todate}, SUM(BLP_AMOUNT), 0) PAID, BLP_USR_ID_A
	FROM (
		SELECT * FROM BILLS INNER JOIN BILLPAYMENTS ON BLL_ID = BLP_ID_BILL 
		WHERE BLL_STATUS <> 'D' 
		AND DATE(BLP_DATE) BETWEEN $P{fromdate} AND $P{todate}
	) PAYMENTS 
GROUP BY BLL_ID, BLP_DATE,  BLL_PAT_NAME
) BILLS 
ORDER BY BLP_USR_ID_A, BLP_DATE]]></queryString>

