	<queryString><![CDATA[SELECT BLL_ID, BLP_DATE,  BLL_PAT_NAME,  REFUNDED, BLP_USR_ID_A
FROM (
	SELECT BLL_ID, BLP_DATE,  BLL_PAT_NAME, IF(DATE(BLP_DATE) BETWEEN $P{fromdate} AND $P{todate}, SUM(ABS(BLP_AMOUNT)), 0) REFUNDED, BLP_USR_ID_A
	FROM (
		SELECT * FROM BILLS bl
		INNER JOIN BILLPAYMENTS ON bl.BLL_ID = BLP_ID_BILL 
		WHERE bl.BLL_STATUS <> 'D' 
		AND bl.BLL_PARENT_ID IS NOT NULL
		AND DATE(BLP_DATE) BETWEEN $P{fromdate} AND $P{todate}
	) PAYMENTS 
GROUP BY BLL_ID, BLP_DATE,  BLL_PAT_NAME
) BILLS 
ORDER BY BLP_USR_ID_A, BLP_DATE]]></queryString>

