        join MEDICALDSRTYPE on MDSR_MDSRT_ID_A = MDSRT_ID_A 
 where 
       INSTR (LOWER(concat(MDSR_ID,MDSR_CODE,MDSR_DESC)) COLLATE utf8_bin, LOWER('$P!{searchtext}')) > 0
 and (($P{typologie}="all") or (($P{typologie}!="all") and (MDSR_MDSRT_ID_A = $P{typologie})))
