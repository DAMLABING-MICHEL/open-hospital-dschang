	<queryString><![CDATA[select  OPD_AGE, OPD_SEX, OPD_NOTE, OPD_DIS_ID_A, OPD_DIS_ID_A_2, OPD_DIS_ID_A_3,
        OPD_PAT_COMPLAINT,
	DATE_FORMAT(OPD_DATE_NEXT_VIS, '%d/%m/%Y') AS OPD_DATE_NEXT_VIS, DATE_FORMAT(OPD_DATE, '%d/%m/%Y') as OPD_DATE,  OPD_ID,
	OPD_DATE_NEXT_VIS as dateTo, OPD_DATE as dateFrom,
        PAT_ID, PAT_NAME,
	PEX_TEMP,PEX_WEIGHT,PEX_HEIGHT,PEX_PA_MAX      
From opd 
	join patient on PAT_ID=OPD_PAT_ID
	left join patientexamination on (OPD_PAT_ID = PEX_PAT_ID and DATE_FORMAT(OPD_DATE, '%d/%m/%Y') = DATE_FORMAT(PEX_DATE, '%d/%m/%Y')
                                         and PEX_ID IN( select  MAX(PEX_ID) from patientexamination 
							where DATE_FORMAT(OPD_DATE, '%d/%m/%Y') = DATE_FORMAT(PEX_DATE, '%d/%m/%Y')
                                                        and OPD_PAT_ID=PEX_PAT_ID
                                                      )
                                        )
WHERE DATE(OPD_DATE) BETWEEN STR_TO_DATE($P{fromdate}, '%d/%m/%Y') and STR_TO_DATE($P{todate}, '%d/%m/%Y')
     
      AND DATEDIFF(OPD_DATE,PAT_BDATE)/30  BETWEEN $P{rangeAgeMin} and $P{rangeAgeMax}
      AND (OPD_NEW_PAT = $P{optionTimeCase} or OPD_NEW_PAT = $P{optionTimeCase1})

Order by OPD_ID]]></queryString>

