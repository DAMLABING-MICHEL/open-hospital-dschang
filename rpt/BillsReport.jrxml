	<queryString><![CDATA[SELECT BLL_ID, BLL_DATE, BLL_UPDATE, BLL_PAT_NAME, BLL_AMOUNT, PAID, IF(DATE(BLL_DATE) BETWEEN $P{fromdate} AND $P{todate}, BLL_AMOUNT - IF(PAID is null,0,PAID), 0) BALANCE, BLL_STATUS,
(SELECT SUM(BLL_AMOUNT) FROM BILLS b1 WHERE b1.BLL_PARENT_ID = BILLS1.BLL_ID) AS REFUNDED_AMOUNT
FROM (
	SELECT BLL_ID, BLL_DATE, BLL_UPDATE, BLL_PAT_NAME, BLL_AMOUNT, IF(DATE(BLP_DATE) BETWEEN $P{fromdate} AND $P{todate}, SUM(BLP_AMOUNT), 0) PAID, BLL_STATUS
	FROM (
		SELECT * FROM BILLS b2 LEFT JOIN BILLPAYMENTS ON b2.BLL_ID = BLP_ID_BILL 
		WHERE b2.BLL_STATUS <> 'D' 
		AND b2.BLL_PARENT_ID IS NULL
		AND (DATE(b2.BLL_DATE) BETWEEN $P{fromdate} AND $P{todate}
		OR DATE(BLP_DATE) BETWEEN $P{fromdate} AND $P{todate})
	) PAYMENTS
GROUP BY BLL_ID
) BILLS1 
ORDER BY BLL_DATE]]></queryString>

