	<queryString><![CDATA[SELECT PR. * , PA.PAT_NAME, VI.PVIS_ID, VI.PVIS_DATE, VI.PVIS_NOTE, 
IF(VI.PVIS_TYPE > 0, 'Postnatal' , 'Prenatal' ) as PVIS_TYPE, EX.PREGEX_DESC, 
RES.PEXRES_OUTCOME, EX.PREGEX_TYPE
FROM PREGNANCY PR
LEFT JOIN PATIENT PA ON PR.PREG_PAT_ID = PA.PAT_ID
RIGHT JOIN PREGNANCYVISIT VI ON VI.PVIS_PREG_ID = PR.PREG_ID
RIGHT JOIN PREGNANCYEXAMRESULT RES ON RES.PEXRES_PVIS_ID = VI.PVIS_ID
JOIN PREGNANCYEXAM EX ON EX.PREGEX_ID = RES.PEXRES_PREGEX_ID
WHERE PREG_ID = $P{pregID}

ORDER BY PREG_NR DESC, PVIS_ID DESC, PREGEX_DESC]]></queryString>

